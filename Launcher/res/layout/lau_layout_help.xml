<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lau__wasu_background" >

    <TextView
        android:id="@+id/help_tx_1"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/sm_115"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="@dimen/sm_60"
        android:gravity="bottom"
        android:text="意见反馈"
        android:textColor="#8555"
        android:textSize="@dimen/sm_60" />

    <ImageView
        android:id="@+id/help_im_0"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/help_tx_1"
        android:layout_marginLeft="@dimen/sm_90"
        android:layout_marginTop="@dimen/sm_25"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_1" />

    <ImageView
        android:id="@+id/help_im_1"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_alignTop="@+id/help_im_0"
        android:layout_marginLeft="@dimen/sm_13"
        android:layout_toRightOf="@+id/help_im_0"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_2" />

    <ImageView
        android:id="@+id/help_im_2"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_alignTop="@+id/help_im_0"
        android:layout_marginLeft="@dimen/sm_13"
        android:layout_toRightOf="@+id/help_im_1"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_3" />

    <ImageView
        android:id="@+id/help_im_3"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_alignTop="@+id/help_im_0"
        android:layout_marginLeft="@dimen/sm_13"
        android:layout_toRightOf="@+id/help_im_2"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_4" />

    <ImageView
        android:id="@+id/help_im_4"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/help_im_0"
        android:layout_marginLeft="@dimen/sm_90"
        android:layout_marginTop="@dimen/sm_13"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_5" />

    <ImageView
        android:id="@+id/help_im_5"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_below="@+id/help_im_0"
        android:layout_marginLeft="@dimen/sm_13"
        android:layout_marginTop="@dimen/sm_13"
        android:layout_toRightOf="@+id/help_im_0"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_6" />

    <ImageView
        android:id="@+id/help_im_6"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_below="@+id/help_im_0"
        android:layout_marginLeft="@dimen/sm_13"
        android:layout_marginTop="@dimen/sm_13"
        android:layout_toRightOf="@+id/help_im_5"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_7" />

    <ImageView
        android:id="@+id/help_im_7"
        style="@style/lau_ImageStyle"
        android:layout_width="@dimen/sm_267"
        android:layout_height="@dimen/sm_190"
        android:layout_below="@+id/help_im_0"
        android:layout_marginLeft="@dimen/sm_13"
        android:layout_marginTop="@dimen/sm_13"
        android:layout_toRightOf="@+id/help_im_6"
        android:padding="@dimen/sm_4"
        android:scaleType="fitXY"
        android:src="@drawable/lau_help_8" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/help_im_6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/sm_20" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="24小时热线: 9 6 3 7 1 "
            android:textColor="#FF000000"
            android:textSize="@dimen/sm_32" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/sm_10"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TVID:0000000000000000000000"
            android:textColor="#FF000000"
            android:textSize="@dimen/sm_33" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="版本号:"
            android:textColor="#FF000000"
            android:textSize="@dimen/sm_33" />
    </LinearLayout>
    
    <com.wasu.launcher.view.TestView
        android:id="@+id/test"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </com.wasu.launcher.view.TestView>    
</RelativeLayout>